<!doctype html>
<html lang="fr">
<head>
    <!-- Required meta tags -->
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <link rel="stylesheet" href="/build/main.css">
    <title>{% block title %}{% endblock %}</title>
</head>
<body>
<div class="body-wrapper">
    <aside class="floating-panel floating-panel-open">
        <nav class="floating-panel-nav">
            <div class="floating-panel-root">
                <a href="#">Front Component</a>
            </div>
        </nav>
        <div class="floating-panel-links">
            <nav>
                <div class="floating-panel-link">
                    <a href="#">Card user</a>
                </div>
            </nav>
        </div>
    </aside>
    <header class="header">
        <div class="header-content">
        <a class="toggler-menu">
            <i class="fas fa-bars"></i>
        </a>
        </div>
    </header>
    <div class="page-wrapper page-wrapper-to-header">
        <main class="content-wrapper">
            <div class="content-body">
                {% block body %}{% endblock %}
            </div>
        </main>
    </div>
</div>

<script src="./build/bundle.js"></script>
</body>
</html>